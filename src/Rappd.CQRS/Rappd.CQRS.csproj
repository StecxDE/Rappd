<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>$(AssemblyName)</PackageId>
		<Title>$(AssemblyName)</Title>
		<Version>1.0.0</Version>
		<Authors>ZeiTee</Authors>
		<Company>Stecx</Company>
		<Product>Rappd</Product>
		<Description>The CQRS part of the Rappd Framework is a fast and simple feature used to implement Commands and Queries in your application.</Description>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageTags>rappd;cqrs</PackageTags>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/StecxDE/Rappd</RepositoryUrl>
		<PackageProjectUrl>https://stecx.de/Rappd</PackageProjectUrl>
		<PackageIcon>Logo_128x128.png</PackageIcon>
		<Copyright>Copyright © Zeitler Andreas 2023</Copyright>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\assets\Logo_128x128.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
